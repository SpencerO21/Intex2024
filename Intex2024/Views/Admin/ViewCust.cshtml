@{
}
@model Intex2024.Models.ViewModels.CustListViewModel


<div class="container">
    <br />
<div class="row">
    <div class="col-md-9">
        <h1>Customers</h1>
    </div>
    <div class="col-md-3" style="text-align: right;">
            <div class="btn btn-outline-info btn-xs">
                <h6><strong><a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="AdminPortal">Back</a></strong></h6>
            </div>
    </div>
</div>


<div class="card m-1 p-1">
    <div class="row">
        <div class="col">
            <h6><strong>Customer Name</strong></h6>
        </div>
        <div class="col">
            <p><strong>Country of Residence</strong></p>
        </div>
        <div class="col">
            <p><strong>Age</strong></p>
        </div>
        <div class="col"></div>
    </div>
</div>


<!-- ... rest of your product listing ... -->
@foreach (Customer c in Model.customers)
{
    <partial name="AdminCardCust" model="c"></partial>
}
<div page-model="@Model.PaginationInfo" page-action="ViewCust" page-classes-enabled="true" page-class="btn"
     page-class-normal="btn-outline-dark" page-class-selected="btn-primary" class="btn-group pull-right m-1"></div>

</div>